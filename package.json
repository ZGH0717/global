  "private": false,
  "scripts": {
    "bootstrap": "npm install ",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "deploy": "npm run deploy:docs && gh-pages -d examples/dist",
    "deploy:docs": "rimraf examples/dist && node build/build.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dist": "npm run clean && npm run build:global-vue && npm run build:global-css",
    "release": "npm run bootstrap && sh build/release.sh",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build:global-vue": "webpack --progress --hide-modules --config build/webpack.build.js && rimraf examples/dist && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.build.min.js",
    "build:global-css": "gulp build --gulpfile packages/global-css/gulpfile.js && rimraf lib/global-css && cp-cli packages/global-css/lib lib/global-css"
  },
